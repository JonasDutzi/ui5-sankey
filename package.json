{
	"name": "@sap/ui5-sankey",
	"version": "0.0.0-development",
	"description": "UI5 library for sankey diagrams",
	"author": "Jonas Dutzi",
	"license": "UNLICENSED",
	"types": "dist/index.d.ts",
	"files": [
		"LICENCE",
		"README.md",
		"tsconfig.json",
		"ui5.yaml",
		"dist",
		"src"
	],
	"scripts": {
		"dev": "ui5 serve --port 8080  --config ui5-test.yaml",
		"test": "playwright test",
		"posttest": "npx nyc merge ./.nyc_output ./.nyc_output/coverage-final.json && npx nyc report --reporter=lcov --report-dir=./coverage",
		"build": "ui5 build --clean-dest --config ui5-build.yaml",
		"build:ts-interfaces": "npx @ui5/ts-interface-generator",
		"start": "ui5 serve -o test-resources/com/sankey/Example.html",
		"start:dist": "npm start -- --config ui5-dist.yaml",
		"testsuite": "ui5 serve -o test-resources/com/sankey/qunit/testsuite.qunit.html",
		"ts-typecheck": "tsc --noEmit",
		"semantic-release": "semantic-release",
		"lint": "oxlint",
		"format": "prettier --write .",
		"format:check": "prettier --check ."
	},
	"devDependencies": {
		"@babel/cli": "^7.28.0",
		"@babel/core": "^7.28.0",
		"@babel/preset-env": "^7.28.0",
		"@babel/preset-typescript": "^7.27.1",
		"@istanbuljs/nyc-config-babel": "^3.0.0",
		"@playwright/test": "^1.54.1",
		"@sapui5/types": "1.130.2",
		"@types/node": "^24.1.0",
		"@ui5/cli": "^4.0.6",
		"@ui5/ts-interface-generator": "^0.9.0",
		"babel-plugin-istanbul": "^7.0.0",
		"dotenv": "^17.2.1",
		"globals": "^15.9.0",
		"nyc": "^17.1.0",
		"oxlint": "^0.15.0",
		"prettier": "^3.3.3",
		"rimraf": "^6.0.1",
		"semantic-release": "^24.2.6",
		"typescript": "^5.5.4",
		"ui5-tooling-modules": "^3.28.1",
		"ui5-tooling-transpile": "^3.5.1"
	},
	"dependencies": {
		"svelte-sankey": "^1.8.4"
	}
}
